{
  "name": "tachyons-measured",
  "version": "1.0.0",
  "description": "A set of higher order components for creating stateless functional UI components using tachyons.",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "repository": "https://github.com/winkerVSbecks/tachyons-measured",
  "author": "Varun Vachhar <varunvachhar@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:umd": "BABEL_ENV=commonjs NODE_ENV=development webpack -p --config webpack.prod.config.js dist/tachyons-measured.js",
    "build:umd:min": "BABEL_ENV=commonjs NODE_ENV=production webpack -p --config webpack.prod.config.js dist/tachyons-measured.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "profile-build": "NODE_ENV=production webpack --config webpack.prod.config.js -p --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "pretest": "npm run lint",
    "test": "NODE_ENV=development jest",
    "lint": "eslint src __tests__",
    "clean": "rimraf lib dist es",
    "prepublish": "npm run clean && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "prop-types": "^15.5.6",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.5.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.0",
    "tachyons": "^4.7.0",
    "webpack": "^2.3.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "tachyons": "^4.6.2"
  },
  "dependencies": {}
}
